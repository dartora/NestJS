{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE"
            ]
        },
        {
            "src": "/swagger(.*)",
            "dest": "node_modules/swagger-ui-dist",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        }
    ]
}